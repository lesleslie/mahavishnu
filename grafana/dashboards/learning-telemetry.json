      {
        "id": 16,
        "title": "Database Size Growth (Last 30 Days)",
        "type": "stat",
        "targets": [
          {
            "expr": "SELECT COUNT(*) as total_rows FROM executions WHERE timestamp >= NOW() - INTERVAL '30 days'",
