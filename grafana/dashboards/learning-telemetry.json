        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "task_type": "Task Type",
                "executions": "Executions",
                "avg_duration": "Avg Duration (s)",
                "avg_quality": "Avg Quality"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
