      {
        "id": 17,
        "title": "Average Routing Confidence (Last 7 Days)",
        "type": "gauge",
        "targets": [
          {
            "expr": "SELECT AVG(routing_confidence) * 100 as avg_confidence FROM executions WHERE routing_confidence IS NOT NULL AND timestamp >= NOW() - INTERVAL '7 days'",
