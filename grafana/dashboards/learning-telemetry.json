          "x": 0,
          "y": 34
        },
        "options": {
          "showHeader": true
        }
      },
      {
        "id": 15,
        "title": "Top Error Types (Last 7 Days)",
        "type": "table",
        "targets": [
          {
            "expr": "SELECT error_type, COUNT(*) as count, MAX(timestamp) as last_occurrence FROM executions WHERE error_type IS NOT NULL AND timestamp >= NOW() - INTERVAL '7 days' GROUP BY error_type ORDER BY count DESC LIMIT 10",
            "legendFormat": "{{error_type}}",
            "refId": "A",
            "datasource": {
              "type": "grafana-simple-datasource",
              "uid": "duckdb-learning"
            },
