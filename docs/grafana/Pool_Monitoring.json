{
  "dashboard": {
    "title": "Mahavishnu Pool Monitoring",
    "description": "Real-time monitoring for Mahavishnu multi-pool orchestration system",
    "tags": ["mahavishnu", "pools", "monitoring"],
    "timezone": "UTC",
    "refresh": "10s",
    "schemaVersion": 35,
    "version": "0.0.1"
    "id": null,
    "uid": "mahavishnu-pool-monitoring"
    "id": null
    "id": null
    "id": null
    "id": null
